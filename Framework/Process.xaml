      <If.Else>
        <Sequence />
      </If.Else>
    </If>
    <ui:FilterDataTable DataTable="[dt_Activities]" DisplayName="Filter by Min Rating" FilterRowsMode="Keep" OutputDataTable="[dt_FilteredActivities]" SelectColumnsMode="Keep">
      <ui:FilterDataTable.Filters>
        <scg:List x:TypeArguments="ui:FilterOperationArgument" Capacity="4">
          <ui:FilterOperationArgument BooleanOperator="And" Operator="GTE">
            <ui:FilterOperationArgument.Column>
