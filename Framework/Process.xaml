    <ui:WriteRange AddHeaders="True" DataTable="[dt_FilteredActivities]" DisplayName="Write Excel Report" SheetName="Sheet1" StartingCell="A1" WorkbookPath="[str_OutputFilePath]" />
    <ui:SendMail Bcc="{x:Null}" Cc="{x:Null}" ContinueOnError="{x:Null}" From="{x:Null}" MailMessage="{x:Null}" Name="{x:Null}" ReplyTo="{x:Null}" Result="{x:Null}" SecurePassword="{x:Null}" TimeoutMS="{x:Null}" UseOAuth="{x:Null}" Body="[&quot;Attached is your travel plan for &quot; + in_TransactionItem(&quot;TargetCity&quot;).ToString]" DisplayName="Send Email" Email="YOUR_EMAIL@GMAIL.COM" EnableSSL="True" IgnoreCRL="False" IsBodyHtml="False" Password="YOUR_APP_PASSWORD" Port="587" SecureConnection="Auto" Server="smtp.gmail.com" Subject="[&quot;Trip Plan: &quot; + in_TransactionItem(&quot;TargetCity&quot;).ToString]" To="[in_TransactionItem(&quot;ClientEmail&quot;).ToString]">
